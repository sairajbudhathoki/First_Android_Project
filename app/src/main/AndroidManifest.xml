<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.first_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/bat"
        android:supportsRtl="true"
        android:theme="@style/Theme.First_Project">
        <activity android:name=".LInk_opner"></activity>
        <activity android:name=".RecyclerView"></activity>
        <activity android:name=".CardViewSpinner" />
        <activity android:name=".List_View" />
        <activity
            android:name=".event_handling"
            android:label="Handling"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Relative_Layout" />
        <activity android:name=".Linear_layout" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>